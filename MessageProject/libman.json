{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@7.0.10",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "vuex@4.1.0",
      "destination": "wwwroot/lib/vuex/"
    },
    {
      "provider": "unpkg",
      "library": "vue-router@4.2.4",
      "destination": "wwwroot/lib/vue-router/"
    },
    {
      "provider": "unpkg",
      "library": "vue@3.3.4",
      "destination": "wwwroot/lib/vue/"
    }
  ]

    
}